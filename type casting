#include <stdio.h>
#include <math.h>  
int main() {
    double a, b, c;   
    int a_int;
    scanf("%lf %lf", &a, &b);
    a_int = (int)a;
    c = a_int + b;
    int d=(int)(c*4.2);
    int k=d*(-1);
    printf("%.3f\n",c);
    printf("-%o",k);
    return 0;
}
//this one is homework of week 3, demanding us to use type casting instead of floor() and ceid() to solve the task.
//A little annoying, it take me half an hour to work it out.
